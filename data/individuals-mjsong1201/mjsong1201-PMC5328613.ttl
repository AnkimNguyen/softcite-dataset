# https://howisonlab.github.io/softcite-pdf-files/pdf-files/pmc_oa_files/PMC5328613.pdf
pmcid:PMC5328613 rdf:type bioj:article ;

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:coded_no_in_text_mentions ;
                           citec:isPresent false; # true/false
                         ] ;
        ] ;


    citec:has_in_text_mention pmcid:PMC5328613_MS01 ; # name in text mention like pmcid:PMC3028497_JC01, no quotes
    citec:has_in_text_mention pmcid:PMC5328613_MS02 ;
.
pmcid:PMC5328613_MS01 rdf:type citec:in_text_mention ; # use in text mention name
    citec:full_quote """At 120 s after SonoVue injection, the QLAB 8.1 software (Philips) was used for dynamic image analysis.""" ; # use triple quotes

    citec:on_pdf_page 2  ; # integer

    citec:spans_pages false ; # true/false

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:mention_type ;
                           rdfs:label  """software"""  ; # put one in triple quotes
                           ca:certainty 10 ; # integer
                           ca:memo """it says software""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:software_was_used ;
                           citec:isPresent true; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:software_name ;
                           citec:isPresent true ; # true/false
                           rdfs:label """QLAB""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:version_number ;
                           citec:isPresent true ; # true/false
                           rdfs:label """8.1""" ; # use triple quotes
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:version_date ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:url ;
                           citec:isPresent false ; # true/false
                         ] ;
        ] ;
    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "mjsong1201" ;
          ca:appliesCode [ rdf:type citec:creator ;
                           citec:isPresent true ; # true/false
                           rdfs:label """Philips""" ; # use triple quotes
                         ] ;
        ] ;
.
