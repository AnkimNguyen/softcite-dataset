.
pmcid:PMC4913573_MZ01 rdf:type citec:in_text_mention ; # use in text mention name
    citec:full_quote """Statistical softwarewas used for calculations and analysis.""" ; # use triple quotes

    citec:on_pdf_page 3  ; # integer

    citec:spans_pages false ; # true/false

    ca:isTargetOf
        [ rdf:type ca:CodeApplication ;
          ca:hasCoder "oceanwall" ;
          ca:appliesCode [ rdf:type citec:mention_type ;
                           rdfs:label """software""" ; # put one in triple quotes
                           ca:certainty 10 ; # integer
                           ca:memo """itm explicitly refers to it as software""" ; # use triple quotes
                         ] ;
        ] ;
